<%- include("partials/header"); -%>
<div class="main-banner">
  <div class="top-text header-text">
  <h2 >Contact Us</h2>
  </div>
  <main class="contact-form">
    <form >
      <div class="form-group row">
        <div class="col-sm-10 col-lg-8 col-md-6">
          <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
        </div>
      </div>
      <br>
      <div class="form-group row">
        <div class="col-sm-10 col-lg-8 col-md-6">
          <textarea type="text" class="form-control" rows="4" cols="80" id="inputPassword3" placeholder="Write to us"></textarea>
        </div>
      </div>

      <div class="form-group row subBtn">
        <div class="col-sm-10">
          <button type="submit" class=" w-50 btn btn-lg btn-dark ">Submit</button>
        </div>
      </div>
    </form>
  </main>
</div>


<%- include("partials/footer"); -%>
